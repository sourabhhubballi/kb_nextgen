+++ b/board/freescale/imx6-sabresd/rootfs_overlay/etc/init.d/S21BootProgress
@@ -0,0 +1,33 @@
+#!/bin/sh
+
+MVE_QT_UPGRADE="/opt/copy_QT_Files_1v0.sh"
+MVE_SIB_UPGRADE="/opt/auto_SIB_Boot_1v0.sh"
+export QT_QPA_PLATFORM=linuxfb:fb=/dev/fb0:size=1024x600:mmSize=1024x600
+
+case "$1" in
+  start)
+  	echo KB BootProgress Check start : OK
+    	# code to start app QT Upgrade  
+    	if [ -x $MVE_QT_UPGRADE ]; then
+       	$MVE_QT_UPGRADE start
+    	fi
+
+    	# code to start to SIB upgrade  
+    	if [ -x $MVE_SIB_UPGRADE ]; then
+        	$MVE_SIB_UPGRADE start
+    	fi
+
+    	# code to start app to boot  
+    	if [ -x $MVE_SIB_UPGRADE ]; then
+        	$MVE_SIB_UPGRADE start
+    	fi  	
+  	;;
+  stop)
+  	echo KB BootProgress Check stop : OK
+  ;;
+  *)
+  echo "Usage: $0 {start|stop}"
+  exit 1
+esac
+
+exit $?
diff --git a/board/freescale/imx6-sabresd/rootfs_overlay/etc/init.d/S23KBQtApp b/board/freescale/imx6-sabresd/rootfs_overlay/etc/init.d/S23KBQtApp
new file mode 100755
index 0000000000..1eaf8a20c3
--- /dev/null
+++ b/board/freescale/imx6-sabresd/rootfs_overlay/etc/init.d/S23KBQtApp
@@ -0,0 +1,33 @@
+#!/bin/sh
+
+export QT_QPA_PLATFORM=linuxfb:fb=/dev/fb0:size=1024x600:mmSize=1024x600
+
+secs=$((10))
+count=$((0))
+
+case "$1" in
+  start)
+	amixer -c sgtl5000fslaudi sset 'Headphone' on  
+        echo KitchenBrian Qt App start : OK
+        while [ $secs -gt 0 ]; do
+                echo Waiting for Bootloader to respond "$secs" ....
+                sleep 1
+                secs=$((secs-1))
+                count=$((count+1))
+                psplash-write "PROGRESS $((90+count))"
+        done	  
+        cd /root/VTC3000QT/VTC3000QT/
+        ./VTC3000QT &
+        cd ~
+        ;;
+  stop)
+        echo KitchenBrian Qt App stop : OK
+        kill -9 $(pidof VTC3000QT)
+        ;;
+  *)
+  echo "Usage: $0 {start|stop}"
+  exit 1
+
+esac
+
+exit $?
diff --git a/board/freescale/imx6-sabresd/rootfs_overlay/etc/init.d/S24KBWireless b/board/freescale/imx6-sabresd/rootfs_overlay/etc/init.d/S24KBWireless
