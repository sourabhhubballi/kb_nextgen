diff --git a/arch/arm/boot/dts/nxp/imx/imx6qdl-nextgen.dtsi b/arch/arm/boot/dts/nxp/imx/imx6qdl-nextgen.dtsi
new file mode 100644
index 000000000000..487a5f4d0e7e
--- /dev/null
+++ b/arch/arm/boot/dts/nxp/imx/imx6qdl-nextgen.dtsi
@@ -0,0 +1,1084 @@
+// SPDX-License-Identifier: GPL-2.0+
+//
+// Copyright 2012 Freescale Semiconductor, Inc.
+// Copyright 2011 Linaro Ltd.
+
+#include <dt-bindings/clock/imx6qdl-clock.h>
+#include <dt-bindings/gpio/gpio.h>
+#include <dt-bindings/input/input.h>
+
+/ {
+	aliases {
+		mxcfb0 = &mxcfb1;
+		mxcfb1 = &mxcfb2;
+		mxcfb2 = &mxcfb3;
+		mxcfb3 = &mxcfb4;
+	};
+
+	chosen {
+		stdout-path = &uart2;
+	};
+
+	memory: memory@10000000 {
+		device_type = "memory";
+		reg = <0x10000000 0x40000000>;
+	};
+
+	reg_3p3v: regulator-3p3v {
+		compatible = "regulator-fixed";
+		regulator-name = "3P3V";
+		regulator-min-microvolt = <3300000>;
+		regulator-max-microvolt = <3300000>;
+		regulator-always-on;
+		gpio = <&gpio6 11 1>;	
+		pinctrl-names = "default";
+		pinctrl-0 = <&pinctrl_lvds>;
+	};
+
+	reg_usb_otg_vbus: regulator-usb-otg-vbus {
+		compatible = "regulator-fixed";
+		regulator-name = "usb_otg_vbus";
+		regulator-min-microvolt = <5000000>;
+		regulator-max-microvolt = <5000000>;
+		gpio = <&gpio3 22 GPIO_ACTIVE_HIGH>;
+		enable-active-high;
+		vin-supply = <&swbst_reg>;
+	};
+
+	reg_usbh1_vbus: regulator@1 {
+		compatible = "regulator-fixed";
+		regulator-name = "usbh1_vbus";
+		regulator-min-microvolt = <5000000>;
+		regulator-max-microvolt = <5000000>;
+		gpio = <&gpio3 31 0>;	
+		pinctrl-names = "default";
+		pinctrl-0 = <&pinctrl_usbh1_vbus>;
+		enable-active-high;
+	};
+
+	mmc_reg: regulator@4 {
+		compatible = "regulator-fixed";
+		regulator-name = "mmc_reg";
+		regulator-min-microvolt = <3300000>;
+		regulator-max-microvolt = <3300000>;
+		regulator-boot-on;
+		regulator-always-on;
+	};
+
+	// power is supplied by VGEN4
+	wlan_en_reg: fixedregulator@5 {
+		compatible = "regulator-fixed";
+		regulator-name = "wlan-en-regulator";
+		regulator-min-microvolt = <1800000>;
+		regulator-max-microvolt = <1800000>;
+		gpio = <&gpio2 7 1>; /* WL_EN (F23)*/
+		startup-delay-us = <70000>;
+		enable-active-low;
+		//regulator-always-on;
+	};
+
+	wlbt_en_reg: fixedregulator@6 {
+		compatible = "regulator-fixed";
+		regulator-name = "wlbt-en-regulator";
+		regulator-min-microvolt = <3300000>;
+		regulator-max-microvolt = <3300000>;
+		gpio = <&gpio2 04 1>; /* WL_EN (A19), uart4 buffer enable*/
+		startup-delay-us = <70000>;
+		enable-active-high;
+		//regulator-always-on;
+	};
+
+	reg_audio: regulator-audio {
+		compatible = "regulator-fixed";
+		regulator-name = "wm8962-supply";
+		gpio = <&gpio4 10 GPIO_ACTIVE_HIGH>;
+		enable-active-high;
+	};
+
+    	reg_hdmi: regulator-hdmi {
+		compatible = "regulator-fixed";
+		regulator-name = "hdmi-5v-supply";
+		regulator-min-microvolt = <5000000>;
+		regulator-max-microvolt = <5000000>;
+		enable-active-high;
+		hdmi-5v-supply = <&swbst_reg>;
+	};
+	
+    	debug-leds {
+		pinctrl-names = "default";
+        	pinctrl-0 = <&debug_leds>;
+		compatible = "gpio-leds";
+		/* debug led1*/
+		debug_led1@1 {
+			label = "debug_led1";
+			gpios = <&gpio6 15 GPIO_ACTIVE_HIGH>;
+			linux,default-trigger = "heartbeat";
+			default-state = "on";
+		};
+		/* debug led2*/
+		debug_led1@2 {
+			label = "debug_led2";
+			gpios = <&gpio6 16 GPIO_ACTIVE_HIGH>;
+			linux,default-trigger = "heartbeat";
+			default-state = "on";
+		};
+	};
+	
+	sound {
+                compatible = "fsl,imx-audio-sgtl5000";
+                model = "sgtl5000-fsl-audio";
+                ssi-controller = <&ssi1>;
+                audio-codec = <&codec>;
+
+                audio-routing =
+                        "Headphone Jack", "HP_OUT",
+                        "Ext Spk", "LINE_OUT";
+                mux-int-port = <1>;
+                mux-ext-port = <5>;
+        };
+	
+#if 0
+	sound-hdmi {
+		compatible = "fsl,imx6qdl-audio-hdmi";
+		model = "imx-audio-hdmi";
+		hdmi-controller = <&hdmi_audio>;
+	};
+#endif
+	mxcfb1: fb@0 {
+		compatible = "fsl,mxc_sdc_fb";
+		disp_dev = "ldb";
+		interface_pix_fmt = "RGB24";
+		default_bpp = <32>;
+		int_clk = <0>;
+		late_init = <0>;
+		status = "disable";
+	};
+
+	mxcfb2: fb@1 {
+		compatible = "fsl,mxc_sdc_fb";
+		disp_dev = "hdmi";
+		interface_pix_fmt = "RGB24";
+		mode_str ="1920x1080M@60";
+		default_bpp = <24>;
+		int_clk = <0>;
+		late_init = <0>;
+		status = "disabled";
+	};
+	
+	mxcfb3: fb@2 {
+		compatible = "fsl,mxc_sdc_fb";
+		disp_dev = "lcd";
+		interface_pix_fmt = "RGB565";
+		mode_str ="CLAA-WVGA";
+		default_bpp = <16>;
+		int_clk = <0>;
+		late_init = <0>;
+		status = "disabled";
+	};
+
+	mxcfb4: fb@3 {
+		compatible = "fsl,mxc_sdc_fb";
+		disp_dev = "ldb";
+		interface_pix_fmt = "RGB666";
+		default_bpp = <16>;
+		int_clk = <0>;
+		late_init = <0>;
+		status = "disabled";
+	};
+	
+	v4l2_cap_0 {
+		compatible = "fsl,imx6q-v4l2-capture";
+		ipu_id = <0>;
+		csi_id = <0>;
+		mclk_source = <0>;
+		status = "disabled";
+	};
+
+	v4l2_cap_1 {
+		compatible = "fsl,imx6q-v4l2-capture";
+		ipu_id = <0>;
+		csi_id = <1>;
+		mclk_source = <0>;
+		status = "disabled";
+	};
+
+	v4l2_out {
+		compatible = "fsl,mxc_v4l2_output";
+		status = "okay";
+	};
+
+	btwilink {
+		compatible = "btwilink";
+	    	status = "okay";
+	};
+	
+	kim {
+		compatible = "kim";
+		nshutdown_gpio = <20>; /* GPIO1_IO20, WILINK_BT_EN */
+		dev_name = "/dev/ttymxc3";
+		flow_cntrl = <1>;
+		baud_rate = <230400>;
+	    	status = "okay";
+	};
+
+	audioclk: ext13Mz {
+                compatible = "pwm-clock";
+                #clock-cells = <0>;
+                clock-frequency = <13200000>;
+                clock-output-names = "audio_pwm1";
+                pwms = <&pwm1 0 75 1>; /* 1 / 75 ns = 13.2 MHz */
+                status = "okay";
+        };
+
+	backlight_lvds1: backlight-lvds1 {
+		compatible = "pwm-backlight";
+		pwms = <&pwm3 0 5000000 1>;
+		brightness-levels = <0 4 8 16 32 64 128 255>;
+		default-brightness-level = <5>;
+		power-supply = <&reg_3p3v>;
+		status = "disabled";
+	};
+
+	backlight_lvds2: backlight-lvds2 {
+		compatible = "pwm-backlight";
+		pwms = <&pwm4 0 5000000 1>;
+		brightness-levels = <0 4 8 16 32 64 128 255>;
+		default-brightness-level = <5>;
+		power-supply = <&reg_3p3v>;
+		status = "disabled";
+	};
+};
+
+&audmux {
+	pinctrl-names = "default";
+	pinctrl-0 = <&pinctrl_audmux>;
+	status = "okay";
+};
+
+&clks {
+	assigned-clocks = <&clks IMX6QDL_CLK_LDB_DI0_SEL>,
+			  <&clks IMX6QDL_CLK_LDB_DI1_SEL>;
+	assigned-clock-parents = <&clks IMX6QDL_CLK_PLL2_PFD0_352M>,
+				 <&clks IMX6QDL_CLK_PLL2_PFD0_352M>;
+};
+
+&dcic1 {
+	dcic_id = <0>;
+	dcic_mux = "dcic-hdmi";
+	status = "disabled";
+};
+
+&dcic2 {
+	dcic_id = <1>;
+	dcic_mux = "dcic-lvds1";
+	status = "disabled";
+};
+
+&ecspi1 {
+	cs-gpios = <&gpio4 9 GPIO_ACTIVE_LOW>;
+	pinctrl-names = "default";
+	pinctrl-0 = <&pinctrl_ecspi1 &pinctrl_ecspi1_cs>;
+	status = "okay";
+
+	flash: flash@0 {
+		#address-cells = <1>;
+		#size-cells = <1>;
+		compatible = "st,m25p32", "jedec,spi-nor";
+		spi-max-frequency = <20000000>;
+		reg = <0>;
+		pinctrl-names ="default";
+		pinctrl-0 = <&pinctrl_norhold>;
+	};
+};
+
+&ecspi4 {
+	pinctrl-names = "default";
+	pinctrl-0 = <&pinctrl_ecspi4 &pinctrl_ecspi4_cs>;
+	fsl,spi-num-chipselects = <1>;
+	cs-gpios = <&gpio3 20 0>;
+	status = "okay";
+};
+
+&fec {
+	pinctrl-names = "default";
+	pinctrl-0 = <&pinctrl_enet>;
+	phy-mode = "rgmii-id";
+	phy-handle = <&phy>;
+	fsl,magic-packet;
+	status = "okay";
+
+	mdio {
+		#address-cells = <1>;
+		#size-cells = <0>;
+
+		phy: ethernet-phy@1 {
+			reg = <1>;
+			qca,clk-out-frequency = <125000000>;
+			reset-gpios = <&gpio1 25 GPIO_ACTIVE_LOW>;
+			reset-assert-us = <10000>;
+		};
+	};
+};
+
+&gpc {
+	fsl,ldo-bypass = <1>;
+};
+
+&hdmi_audio {
+	status = "disabled";
+};
+
+&hdmi_cec {
+	pinctrl-names = "default";
+	pinctrl-0 = <&pinctrl_hdmi_cec>;
+	status = "disabled";
+};
+
+&hdmi_core {
+	ipu_id = <0>;
+	disp_id = <0>;
+	status = "disabled";
+};
+
+&hdmi_video {
+	fsl,phy_reg_vlev = <0x294>;
+	fsl,phy_reg_cksymtx = <0x800d>;
+	HDMI-supply = <&reg_hdmi>;
+	status = "disabled";
+};
+
+&i2c1 {
+	clock-frequency = <100000>;
+	pinctrl-names = "default";
+	pinctrl-0 = <&pinctrl_i2c1>;
+	status = "okay";
+	
+	/* mmpf0100 */
+	pmic: pfuze100@8 {
+		compatible = "fsl,pfuze100";
+		reg = <0x08>;
+		
+		regulators {
+			/* VDD_CORE, 1.375 */
+			sw1a_reg: sw1a {
+				regulator-min-microvolt = <1375000>;
+				regulator-max-microvolt = <1375000>;				
+				regulator-boot-on;
+				regulator-always-on;
+				regulator-ramp-delay = <6250>;
+			};
+			
+			// sw1b, not sure it is used, schematic has posible error
+			
+			/* VDD_SOC, 1.375 */
+			sw1c_reg: sw1c {
+				regulator-min-microvolt = <1375000>;
+				regulator-max-microvolt = <1375000>;				
+				regulator-boot-on;
+				regulator-always-on;
+				regulator-ramp-delay = <6250>;
+			};
+			
+			/* VDD_HIN, 3.3 */
+			sw2_reg: sw2 {
+				regulator-min-microvolt = <3300000>;
+				regulator-max-microvolt = <3300000>;
+				regulator-boot-on;
+				regulator-always-on;
+			};
+			
+			/* VDD_DDR, 1.5 */
+			sw3a_reg: sw3 {
+				regulator-min-microvolt = <1500000>;
+				regulator-max-microvolt = <1500000>;
+				regulator-boot-on;
+				regulator-always-on;
+			};
+
+			/* sw3b not used */
+
+			/* AUX_3V15, 3.15 */
+			sw4_reg: sw4 {
+				regulator-min-microvolt = <3150000>;
+				regulator-max-microvolt = <3150000>;
+				regulator-always-on;
+			};			
+
+			/* PMIC_5V, 5.0 */ 
+			swbst_reg: swbst {
+				regulator-min-microvolt = <5000000>;
+				regulator-max-microvolt = <5000000>;
+				regulator-always-on;
+			};
+
+			/* PMIC_VSNVS */
+			snvs_reg: vsnvs {
+				regulator-min-microvolt = <1000000>;
+				regulator-max-microvolt = <3000000>;
+				regulator-boot-on;
+				regulator-always-on;
+			};
+
+			/* DDR_VREF, 0.75 */
+			vref_reg: vrefddr {
+				regulator-boot-on;
+				regulator-always-on;
+			};
+
+			/* VGEN1 1.5 @ 100mA */
+			vgen1_reg: vgen1 {
+				regulator-min-microvolt = <1500000>;
+				regulator-max-microvolt = <1500000>;
+				regulator-always-on;
+			};
+			
+			/* VGEN2 1.5 @ 250mA */
+			vgen2_reg: vgen2 {
+				regulator-min-microvolt = <1500000>;
+				regulator-max-microvolt = <1500000>;
+				regulator-always-on;				
+			};
+
+			/* VGEN3 2.5 @ 100mA */
+			vgen3_reg: vgen3 {
+				regulator-min-microvolt = <2500000>;
+				regulator-max-microvolt = <2500000>;
+				regulator-always-on;
+			};
+
+			/* VGEN4 1.8 @ 350mA */
+			vgen4_reg: vgen4 {
+				regulator-min-microvolt = <1800000>;
+				regulator-max-microvolt = <1800000>;
+				regulator-always-on;
+			};
+			
+			/* VGEN5 2.8 @ 100mA */
+			vgen5_reg: vgen5 {
+				regulator-min-microvolt = <2800000>;
+				regulator-max-microvolt = <2800000>;
+				regulator-always-on;
+			};
+			
+			/* VGEN6 3.3 @ 200mA */
+			vgen6_reg: vgen6 {
+				regulator-min-microvolt = <3300000>;
+				regulator-max-microvolt = <3300000>;
+				regulator-always-on;
+			};
+		};
+	};
+
+	touch_i2c: atmel_mxt_ts@4a {
+		compatible = "atmel,atmel_mxt_ts";
+		reg = <0x4a>;
+		interrupt-parent = <&gpio2>;
+		interrupts = <0 IRQ_TYPE_EDGE_FALLING>;
+   	};
+
+	ds1340: rtc@68 {
+                compatible = "dallas,ds1340";
+                reg = <0x68>;
+                trickle-resistor-ohms = <250>;
+                trickle-diode-disable;
+  	};
+};
+
+&i2c2 {
+	clock-frequency = <100000>;
+	pinctrl-names = "default";
+	pinctrl-0 = <&pinctrl_i2c2>;
+	status = "okay";
+
+#if 0
+	hdmi_edid: edid@50 {
+		compatible = "fsl,imx6-hdmi-i2c";
+		reg = <0x50>;
+	};
+#endif
+};
+
+&i2c3 {
+	clock-frequency = <400000>;
+	pinctrl-names = "default";
+	pinctrl-0 = <&pinctrl_i2c3>;
+	status = "okay";
+
+	// 400KHz
+	codec: sgtl5000@a {
+		compatible = "fsl,sgtl5000";
+		reg = <0x0a>;
+		clocks = <&audioclk>;
+		clock-names = "sgtl_clk";
+        // for 24MHz clock, the one going to mipi camera (not used)
+        //clocks = <&clks IMX6QDL_CLK_CKO>;
+		VDDA-supply = <&vgen4_reg>;			// 1v8
+		VDDIO-supply = <&sw2_reg>;			// 3v3
+		VDDD-supply = <&vgen1_reg>;			// 1v5
+	};
+
+	// no driver, needs app, should have an i2c in /dev/i2c3
+	// eeram 1MHz, 0xa0 - 0xaC, A1, A2 = 1, 0x56 (left shift 1)
+
+	/* eeprom, 47L16 */
+	eeram: 47L16@56 {
+                compatible = "24c16";
+                reg = <0x56>;
+
+                // ERAM_HS, gpio2-6
+                // hardware stroe
+        };
+	
+};
+
+&i2c4 {
+	clock-frequency = <400000>;
+	pinctrl-names = "default";
+	pinctrl-0 = <&pinctrl_i2c4>;
+	status = "okay";
+};
+
+&iomuxc {
+	pinctrl-names = "default";
+	pinctrl-0 = <&pinctrl_hog>;
+
+	imx6q-nextgen {
+		pinctrl_hog: hoggrp {
+			fsl,pins = <
+				// AUX Connector, supply enable for regulator
+				MX6QDL_PAD_GPIO_2__GPIO1_IO02			0x80000000
+				// AUX Connector
+				MX6QDL_PAD_GPIO_4__GPIO1_IO04			0x80000000
+				// aux con
+				MX6QDL_PAD_GPIO_19__GPIO4_IO05			0x80000000
+				// GPS_PWREN
+				MX6QDL_PAD_ENET_RXD0__GPIO1_IO27		0x80000000
+				// fingerprint reset
+				MX6QDL_PAD_SD4_DAT7__GPIO2_IO15			0x80000000
+				// bar irq
+				MX6QDL_PAD_NANDF_D1__GPIO2_IO01			0x80000000
+				// acc irq
+				MX6QDL_PAD_NANDF_D2__GPIO2_IO02			0x80000000
+				// als irq, light sensor
+				MX6QDL_PAD_NANDF_D3__GPIO2_IO03			0x80000000
+				// eeprom wp
+				MX6QDL_PAD_NANDF_D5__GPIO2_IO05			0x80000000
+				// eram hs, force hw stgore event
+				MX6QDL_PAD_NANDF_D6__GPIO2_IO06			0x80000000
+				//  ac presence
+				MX6QDL_PAD_SD4_DAT4__GPIO2_IO12			0x80000000
+				// ac wave
+				MX6QDL_PAD_SD4_DAT5__GPIO2_IO13			0x80000000
+				// sata sleep (configured for lvds vs lcd24 display select)
+				MX6QDL_PAD_NANDF_CLE__GPIO6_IO07		0x80000000
+				// pmic int, driver does not use an interrupt
+				MX6QDL_PAD_GPIO_18__GPIO7_IO13			0x80000000
+				// TP8
+				MX6QDL_PAD_CSI0_MCLK__GPIO5_IO19		0x80000000
+			>;
+		};
+		
+		pinctrl_audmux: audmuxgrp {
+			fsl,pins = <
