+
+/dts-v1/;
+
+#include "imx6dl.dtsi"
+#include "imx6qdl-nextgen.dtsi"
+
+/ {
+	model = "Freescale i.MX6 DualLite KBs platform Board";
+	compatible = "fsl,imx6dl-sabresd", "fsl,imx6dl";
+};
+
